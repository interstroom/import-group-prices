    "license":"OSL-3.0",
    "description":"Simple and extensible import tool to allow you to set multiple group prices to magento products",
    "authors":[
        {
            "name":"ryaan-anthony",
            "email":"ryan@inverseparadox.net"
        },
        {
            "name":"Van der Meer",
            "email":"vandermeer@interstroom.nl"
        }
    ],
    "require":{
        "magento-hackathon/magento-composer-installer":"*"
    },
    "extra": {
        "map": [
            ["app/design/adminhtml/default/default/template/import", "app/design/adminhtml/default/default/template/import"],
            ["app/design/adminhtml/default/default/layout/import.xml", "app/design/adminhtml/default/default/layout/import.xml"],
